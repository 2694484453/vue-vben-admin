{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [".env.local"],
  "globalEnv": ["NODE_ENV"],
  "pipeline": {
    "build": {
      "cache": false,
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "stub": {},
    "lint": {},
    "clean": {
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
